    }
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts --max_old_space_size=8192 start",
    "wstart": "npm run build-css && cross-env npm-run-all -p watch-css start-js",
    "start": "react-scripts --max_old_space_size=8192 start",
    "build": "react-scripts --max_old_space_size=8192 build",
    "test": "NODE_PATH=./src jest --setupTestFrameworkScriptFile=raf/polyfill",
    "wtest": "jest",
    "eject": "react-scripts eject",
    "clean": "rm -rf ./node_modules/.cache",
    "rocketize": "bash ./scripts/rocketize-webpack.sh"
  },
  "jest": {
    "setupFiles": [
      "jest-localstorage-mock"
    ]
  },
